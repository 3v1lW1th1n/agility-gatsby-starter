    "react-dom": "^16.8.6",
    "truncate-html": "^1.0.2"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agility/agility-gatsby-starter"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
